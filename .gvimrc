""" Interface
colorscheme monokai
set guioptions=acir
set browsedir=current

" Fonts
set guifont=DejaVu\ Sans\ Mono\ 10 
set linespace=0
" Window size
set columns=120
set lines=45
" Special chars
set list


""" Keyboard / Mouse
set mousemodel=extend
set mousefocus
set winaltkeys=no
